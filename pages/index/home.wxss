}

.spCarBox {
  position: fixed;
  z-index: 88;
  top: 0;
  left: 0;
  right: 0;
  bottom: 100rpx;
  background: rgba(0, 0, 0, 0.5);
  /* display: none; */
}

.spCarBox-content {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  background: #fff;
}

.spCarBox-tool {
  background: #e6e6e6;
  height: 50rpx;
  padding: 0 18rpx;
}

.spCarBox-toolLi {
  font-size: 26rpx;
  color: #999;
  line-height: 40rpx;
}

.spCarBox-toolLi image {
  width: 26rpx;
  height: 31rpx;
  margin-right: 16rpx;
}

.spCarBox-sc {
  max-height: 452rpx;
  overflow-y: auto;
}

.spCarBox-empty {
  font-size: 28rpx;
  color: #a1a1a1;
  text-align: center;
  line-height: 140rpx;
}

.spCarBox-scLi {
  height: 120rpx;
  border-bottom: 2rpx solid #eee;
  margin: 0 30rpx;
}

.spCarBox-scFood {
  font-size: 30rpx;
  color: #000;
  line-height: 34rpx;
}

.spCarBox-scPrice {
  font-size: 30rpx;
  color: #000;
  line-height: 34rpx;
  width: 160rpx;
  text-align: center;
}

.spCarBox-scSub {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 2rpx solid #d6d6d6;
  position: relative;
  box-sizing: border-box;
}

.spCarBox-scSub::before {
  content: '';
  width: 24rpx;
  height: 4rpx;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background: #d6d6d6;
}

.spCarBox-scAdd {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  border: 2rpx solid #c79f62;
  position: relative;
  box-sizing: border-box;
  background: #c79f62;
}

.spCarBox-scAdd::before, .spCarBox-scAdd::after {
  content: '';
  width: 24rpx;
  height: 4rpx;
  border-radius: 10rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  background: #fff;
}

.spCarBox-scAdd::after {
  width: 4rpx;
  height: 24rpx;
}

.spCarBox-scInput {
  width: 80rpx;
}

.spCarBox-scInput input {
  width: 100%;
  height: 42rpx;
  line-height: 42rpx;
  font-size: 30rpx;
  text-align: center;
}

