  position: relative;
  background: #f5f5f5;
  border-top: 2rpx solid #eee;
}

.sifBox-tab {
  height: 76rpx;
  background: #fff;
  padding: 0 24rpx;
  box-shadow: 0 2rpx 4rpx 0 rgba(0, 0, 0, 0.2);
  z-index: 66;
  position: relative;
  width: 100%;
}

.sifBox-tabLi {
  line-height: 76rpx;
  font-size: 28rpx;
  color: #000;
  margin-right: 100rpx;
  position: relative;
}

.sifBox-tabLi.cur::before {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 4rpx;
  background: #c79f62;
  border-radius: 8rpx;
}

.sifBox-content {
  position: absolute;
  left: 0;
  right: 0;
  top: 76rpx;
  bottom: 0;
  padding-top: 20rpx;
  overflow-y: auto;
  /* display: none; */
}

.sifBox-content.pt0 {
  padding-top: 0;
  background: #fff;
}

.sifBox-cmtTop {
  height: 150rpx;
  background: #fff;
  margin-bottom: 20rpx;
}

.sifBox-cmtScore, .sifBox-cmtLoc {
  width: 172rpx;
  text-align: center;
}

.sifBox-cmtScore em, .sifBox-cmtLoc em {
  font-size: 50rpx;
  line-height: 54rpx;
  color: #fb3434;
  display: block;
}

.sifBox-cmtScore span, .sifBox-cmtLoc span {
  margin-top: 10rpx;
  font-size: 24rpx;
  line-height: 28rpx;
  color: #333;
  display: block;
}

.sifBox-cmtPut {
  overflow: hidden;
}

.sifBox-cmtStar {
  padding: 12rpx 0;
}

.sifBox-cmtStar span {
  width: 66rpx;
  font-size: 24rpx;
  color: #333;
  line-height: 28rpx;
}

.sifBox-cmtStar image {
  width: 24rpx;
  height: 24rpx;
  margin: 0 10rpx;
}

.sifBox-cmtStar em {
  margin-left: 6rpx;
  font-size: 24rpx;
  color: #fb3434;
  line-height: 28rpx;
}

.sifBox-cmtLoc {
  border-left: 2rpx solid #ccc;
}

.sifBox-cmtLoc em {
  color: #999;
}

.sifBox-cmtLable {
}

.sifBox-cmtLableLi {
}

