  border-bottom: 2rpx solid #dcdddd;
}

.sifBox-cmtPerson {
}

.sifBox-cmtHead {
  border-radius: 50%;
  width: 60rpx;
  height: 60rpx;
  margin-right: 18rpx;
}

.sifBox-cmtInfo {
  overflow: hidden;
}

.sifBox-cmtName {
  font-size: 28rpx;
