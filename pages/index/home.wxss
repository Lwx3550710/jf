  height: 150rpx;
  float: left;
  margin-right: 16rpx;
  margin-bottom: 16rpx;
  border: 2rpx solid #eee;
}

.sifBox-spAdd, .sifBox-spFood, .sifBox-spTime, .sifBox-spLoc {
  height: 68rpx;
  background: #fff;
  padding: 0 24rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
