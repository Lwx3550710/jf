.container{
  background: #f2f2f2;
  width: 100%;
  position: absolute;
  height: 100%;

}
.card-main{
    width: 323px;
    height: 480px;
    position: relative;
    z-index: 1;
    margin: 15px auto 0;
    overflow: hidden;
}
.card-main .body{
    position: absolute;
    z-index: 2;
    width: 100%;
    height: 100%;
}
.card-main .title{
    line-height: 93px;
    text-align: center;
    font-size: 20px;
}
.card-main .process{
    text-align: center;
    padding: 10px 0;
    overflow: hidden;
}
.card-main .process .no{
    color: #C79F62;
    font-size: 45px;
    line-height: 60px;
}
.card-main .process .jd{
    color: #000;
    font-size: 15px;
    line-height: 30px;
}
.card-main .process .store{
    color: #000;
    font-size: 16px;
    line-height: 30px;
}
.card-main .process .store b{
    display: inline-block;
    margin-right: 3px;
}
.card-main .process .store image{
    width: 10px;
    height: 10px;
}
.card-main .info{
padding: 0 20px 20px;
overflow: auto;
height: 304rpx;

}
.card-main .info .item{
    width: 100%;
    margin-top: 15px;
    overflow: hidden;
}
.card-main .info .item .box1{
    line-height: 30px;
}
.card-main .info .item .box1 .name{
    color: #000;
    font-size: 15px;
    float: left;
}
.card-main .info .item .box1 .spec{
    color: #666;
    font-size: 12px;
    float: right;
    margin-right: 15px;
}
.card-main .info .item .box1 .price{
    color: #000;
    font-size: 15px;
    float: right;
}
.card-main .info .item .box2{
    color: #666;
    font-size: 12px;
    line-height: 20px;
    clear: both;
}
.card-main .info .item:last-child{
    margin-bottom: 0;
}
.card-main .info .more{
    width: 100%;
    line-height: 40px;
    margin:5px 0 10px;
    overflow: hidden;
}
.card-main .info .more dt{
    color: #C79F62;
    font-size: 13px;
    float: left;
}
.card-main .info .more dd{
    float: right;
    width: 9px;
    height: 9px;
}
.card-main .info .more dd image{
    width: 9px;
    height: 9px;
}
.card-main .total{
    margin: 0 20px;
    border-top: 1px #ccc dashed;
    overflow: hidden;
}
.card-main .total dt{
    font-size: 14px;
    line-height: 60px;
    float: left;
}
.card-main .total dd{
    font-size: 18px;
    line-height: 60px;
    font-weight: bold;
    float: right;
}
.card-main .bg{
    position: absolute;
    z-index: 1;
    width: 100%;
    height: 100%;
}
.card-main .bg image{
    width: 100%;
    height: 100%;
}
.copyright{
    color: #999;
    font-size: 12px;
    text-align: center;
    padding: 15px 0;
}
.wm-head{
  width:80rpx;
  height: 80rpx;
  border-radius: 50%;
  border:2rpx solid #ddd;
}
.wm-name {

}
.wm-name text{
color: #000000;
font-size: 30rpx;
margin: 5px 12px 10rpx 0rpx;
padding-left: 134rpx;

}
.wm-name image{
  width: 100rpx;
  height: 30rpx;
}
.wm-info{
  font-size: 26rpx;
  color: #000000;

}
.wm-contact{
  font-size: 28rpx;
  color:#C79F62;
  width:180rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  border-radius: 30rpx;
  border: 2rpx solid #C79F62;
  margin: 25rpx auto 0;

}
.qucan-conent{
position: absolute;
z-index: 1;
text-align: center;
width: 100%;

}
.qucan-people{
width: 150rpx;
margin-top: 264rpx;

}
.qucan-tips{
display: block;
font-size: 26rpx;
margin-top: 41rpx;
color: #999;

}
.qucan-btn{
width: 300rpx;
height: 60rpx;
text-align: center;
line-height: 60rpx;
background: #C79F62;
color: #fff;
border-radius: 10rpx;
margin: 250rpx auto 0;
font-size: 30rpx;

}